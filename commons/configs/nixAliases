# unix commands
alias ll='ls -al'
alias ..='cd ..'
alias ..='cd ..'
alias ...='cd ../../../'
alias ....='cd ../../../../'
alias .....='cd ../../../../'
alias .4='cd ../../../../'
alias .5='cd ../../../../..'


# git aliases 
alias gstatus='git status'
alias gaddem='git add .'
alias gadd='git add'
alias grm='git rm'
alias gcommit='git commit -am'
alias gpush='git push'
alias gpushnewbranch='git push --set-upstream origin'
alias gpull='git pull'
alias ghistory='git log --graph --pretty=oneline --abbrev-commit'
alias ghistorypretty='git log --pretty=oneline'
alias gconfig='git config --list'
alias gconfigname='git config user.name'
alias gconfigemail='git config user.email'
alias gdiff='git diff'
alias grevert='git reset --hard'
alias gfetch='git fetch origin'
alias gbranch='git branch'
alias gbranches='git branch -a'
alias gcoutbranch='git checkout -b'
alias ghistory='git log --graph --pretty=oneline --abbrev-commit'
alias ghistorypretty='git log --pretty=oneline'
alias gCommitEmpty='git commit --allow-empty -m "empty commit"'
alias gcreatetag='git tag'
alias gpushalltags='git push --tags'
alias gdeletetag='git tag -d'
alias gpushtags='git push origin :refs/tags'

# files navigation
alias workspace='cd $WORKSPACE_PATH'
alias configos='cd $WORKSPACE_PATH/config-os'
alias notes='cd $WORKSPACE_PATH/notes'
alias reactjs='cd $WORKSPACE_PATH/js-playground'
alias nodejs='cd $WORKSPACE_PATH/js-playground'
alias android='cd $WORKSPACE_PATH/android-playground'
alias kotlinWorkspace='cd $WORKSPACE_PATH/kotlin-playground'
alias scalaWorkspace='cd $WORKSPACE_PATH/scala-playground'

alias deleteAllSSHKeys='ssh-add -D'
alias showAllSSHKeys='ssh-add -l'
alias addpersonalsshkey='ssh-add -D && ssh-add ~/.ssh/id_rsa_personal'
alias addworksshkey='ssh-add -D && ssh-add ~/.ssh/id_rsa_work'